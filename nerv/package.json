{
  "name": "vern",
  "version": "1.0.0",
  "type": "module",
  "description": "VERN Stack - Vite + Express + React + Node.js template",
  "main": "Main.jsx",
  "scripts": {
    "dev": "vite",
    "start": "vite preview --port 5000 --host 0.0.0.0",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx --fix",
    "clean": "rimraf dist",
    "deploy:vercel": "npm run build && vercel --prod",
    "deploy:github": "npm run build && gh-pages -d dist",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "keywords": [
    "vern",
    "vite",
    "react",
    "nodejs",
    "express",
    "serverless",
    "vercel",
    "template"
  ],
